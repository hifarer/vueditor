{
  "name": "vueditor",
  "version": "0.2.5",
  "description": "a wysiwyg editor based on Vue.js and Vuex",
  "repository": {
    "type": "git",
    "url": "https://github.com/hifarer/vueditor.git"
  },
  "main": "dist/js/vueditor.min.js",
  "scripts": {
    "dev-server": "node ./build/dev-server.js",
    "dev": "webpack --config ./build/webpack.dev.config.js  --progress --hide-modules",
    "build": "webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "plugin": "webpack --config ./build/webpack.plug.config.js --progress --hide-modules"
  },
  "dependencies": {
    "marked": "^0.3.6",
    "twemoji": "^2.3.0",
    "vue": "^2.0.5",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.7.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "postcss": "^5.2.8",
    "postcss-loader": "^1.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.9.5",
    "vue-template-compiler": "2.0.5",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "keywords": [
    "vue editor",
    "vue.js editor",
    "rich text editor",
    "wysiwyg editor",
    "vue.js wysiwyg editor"
  ],
  "author": "hifarer",
  "license": "MIT"
}
